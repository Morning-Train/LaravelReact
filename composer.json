{
  "name": "morningtrain/laravel-react",
  "type": "library",
  "description": "A context helper package for Laravel",
  "homepage": "http://morningtrain.dk/",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Bjarne Bonde",
      "email": "bb@morningtain.dk",
      "homepage": "https://morningtrain.dk/",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "MorningTrain\\Laravel\\React\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1",
    "nesbot/carbon": "^2.16",
    "guzzlehttp/guzzle": "^6.3",
    "morningtrain/laravel-support": "^1.0",
    "morningtrain/laravel-context": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MorningTrain\\Laravel\\React\\ReactServiceProvider"
      ],
      "aliases": {
        "React": "MorningTrain\\Laravel\\React\\React"
      }
    }
  }
}
